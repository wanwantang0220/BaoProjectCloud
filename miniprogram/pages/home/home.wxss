.contain {
  width: 100%;
  height: 100%;
}

.view_search {
  display: flex;
  flex: row;
  height: 80rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
  margin-top: 10rpx;
  border: 1rpx solid #d81e06;
  border-width: 1rpx;
  border-radius: 40rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  align-items: center;
}

.input_search {
  font-size: 24rpx;
  width: 500rpx;
  height: 80rpx;
}

.line {
  width: 1rpx;
  height: 60rpx;
  margin: 15rpx;
  background: #d81e06;
}

.image_search {
  width: 50rpx;
  height: 50rpx;
  padding-bottom: 10rpx;
  padding-top: 10rpx;
  padding-left: 10rpx;
  padding-right: 20rpx;
}

.view_swiper {
  /* display: block; *//* flex-direction: column; *//* background: #fff; */
  display: flex;
  flex: row;
  width: 100%;
  height: 350rpx;
  margin-top: 20rpx;
  align-items: center;
  justify-content: center;
}

.swiper {
  width: 100%;
  height: 350rpx;
  background: #fff;
  align-items: center;
  justify-content: center;
}

.view_center {
  width: 100%;
  height: 350rpx;
}

.image {
  width: 100%;
  height: 100%;
}

.view_cate {
  width: 100%-80rpx;
  display: flex;
  flex-direction: row;
  margin:20rpx 40rpx;
  justify-content: space-around;
}

.view_cate_item {
  display: flex;
  flex-direction: column;
  margin: 20rpx;
  align-items: center;
  justify-content: center;
}

.view_cate_item_image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.view_cate_item_text{
  font-size: 24rpx;
  color: #333333;
  padding: 10rpx;
}
