.contain {
  width: 750rpx;
  background: #fff;
}

.view_input {
  width: 750rpx;
  display: flex;
  flex-direction: row;
  margin: 10rpx 20rpx;
  height: 52px;
  line-height: 52px;
  font-size: 34rpx;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}

.input {
  margin-left: 20rpx;
  align-items: center;
  height: 52px;
  line-height: 52px;
}

.view_commit {
  display: flex;
  width: 750rpx;
  height: 52px;
  line-height: 52px;
  margin-top: 100rpx;
  background: #007aff;
  /* border-radius: 50rpx; */
  align-items: center;
  justify-content: center;
}
.uploader-text, .tunnel-text {
  width: 100%;
  line-height: 52px;
  font-size: 34rpx;
  color: #007aff;
}

.uploader-container {
  width: 100%;
  height: 400rpx;
  padding: 20rpx 20rpx 20rpx 0;
  display: flex;
  align-content: center;
  justify-content: center;
  box-sizing: border-box;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
}

.uploader-image {
  width: 100%;
  height: 360rpx;
}